@{
    Layout = null;

}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title></title>
</head>
<body>
    <div>
        <h1>@ViewBag.seriestTitle</h1>
        <h2>号码 #@ViewBag.IssueNumber</h2>

        <h5>介绍：</h5>
        <div>@Html.Raw(ViewBag.Description)</div>
        @if (ViewBag.Artists.Length > 0)
        {
            <h5>作者：</h5>
            <div>
                <ul>
                    @foreach (var artist in ViewBag.Artists)
                    {

                        <li>
                            @artist
                        </li>
                    }
                </ul>
            </div>
        }
    </div>
</body>
</html>